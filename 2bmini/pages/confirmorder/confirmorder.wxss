page{
    background: #f4f4f4;
}
.container {
    justify-content: initial;
  }
  .no-goods-icon {
    width: 100rpx;
    height: 100rpx;
    margin-top: 300rpx;
  }
  .title-box {
    width: 100vw;
    margin-top: 30rpx;
    text-align: center;
    font-size: 28rpx;
    color: #999;
    margin-bottom: 50rpx;
  }
  
  .to-index-btn {
    color: #fff;
    background: #e64340;
    border-radius: 6px;
    width: 300rpx;
    height: 70rpx;
    line-height: 70rpx;
    text-align: center;
    font-size: 28rpx;
  }
  
  .list-top {
    width: 100%;
    height: 88rpx;
    line-height: 88rpx;
    box-sizing: border-box;
    display: flex;
    justify-content: space-between;
    padding: 0 30rpx;
    font-size: 28rpx;
    align-items: center;
  }
  
  .list-top .label {
    color: #000;
  }
  
  .list-top .edit-btn {
    color: #999;
    height: 100%;
  }
  
  .goodsList {
    width: 100%;
   
    padding-bottom: 100rpx;
  }
  
  .a-gooods {
    width: 100%;
    overflow: hidden;
    box-sizing: border-box;
  }
  
  .a-goods-conts {
    display: flex;
    justify-content: space-between;
    width: 100%;
    box-sizing: border-box;
    transition: margin-left 0.2s ease-in-out;
    margin-bottom: 30rpx;
    background-color: #fff;
  }
  
  .goods-info {
    border-bottom: 1px solid #eee;
    display: flex;
    padding: 30rpx 0 30rpx 0;
    width:100%;
    box-sizing: border-box;
    align-items: center;
    margin-left: 20rpx;
    justify-content: space-between;
    position: relative;
  
  }
  
  .goods-info .img-box {
    width: 140rpx;
    height: 140rpx;
    overflow: hidden;
    margin-right: 20rpx;
    background-color: #d8d8d8;
    flex-shrink: 0;
    margin-left: 30rpx;
  }
  
  .goods-info .text-box {
    width: 70%;
    display: flex;
    justify-content: space-around;
  }
  
  .goods-info .text-box .goods-title {
    width: 500rpx;
    font-size: 28rpx;
    color: #000;
    text-overflow: ellipsis;
    white-space: nowrap;
    overflow: hidden;
    padding: 10rpx 20rpx 5rpx 0;
  }
  
  .goods-info .text-box .goods-label {
    font-size: 26rpx;
    color: #999;
    height: 38rpx;
    line-height: 38rpx;
    margin: 8rpx 0 25rpx 0;
  }
  
  .goods-info .text-box .goods-price {
    font-size: 24rpx;
    color: #e64340;
  }
  
  .goods-info .text-box .buy-num {
    width: 164rpx;
    height: 48rpx;
    line-height: 48rpx;
    display: flex;
    /*justify-content: space-between;*/
    font-size: 24rpx;
    text-align: center;
  }
  
  .goods-info .text-box .buy-num .jian-btn {
    width: 48rpx;
    height: 100%;
    border-left: 1rpx solid #ccc;
    border-bottom: 1rpx solid #ccc;
    border-top: 1rpx solid #ccc;
    border-bottom-left-radius: 6rpx;
    border-top-left-radius: 6rpx;
  }
  
  .goods-info .text-box .buy-num .jian-btn.disabled {
    background-color: #f5f5f9;
    border-left: 1rpx solid #eee;
    border-bottom: 1rpx solid #eee;
    border-top: 1rpx solid #eee;
    color: #ccc;
  }
  
  .goods-info .text-box .buy-num .jia-btn {
    width: 48rpx;
    height: 100%;
    border-right: 1rpx solid #ccc;
    border-bottom: 1rpx solid #ccc;
    border-top: 1rpx solid #ccc;
    border-bottom-right-radius: 6rpx;
    border-top-right-radius: 6rpx;
  }
  
  .goods-info .text-box .buy-num .jia-btn.disabled {
    background-color: #f5f5f9;
    border-right: 1rpx solid #eee;
    border-bottom: 1rpx solid #eee;
    border-top: 1rpx solid #eee;
    color: #ccc;
  }
  
  .goods-info .text-box .buy-num input {
    width: 68rpx;
    height: 48rpx;
    min-height: 48rpx;
    text-align: center;
    font-size: 24rpx;
    border: 1rpx solid #ccc;
  }
  
  .goods-info .img-box .img {
    width: 140rpx;
    height: 140rpx;
  }
  
  .a-goods-conts .delete-btn {
    width: 120rpx;
    line-height: 220rpx;
    text-align: center;
    background: #e64340;
    font-size: 24rpx;
    color: #fff;
  }
  
  .next {
    width: 32rpx;
    height: 32rpx;
    background: #fff;
  }
  .wrap-click-box{
    border: 1px solid #ccc;
    width: 40rpx;
    height: 40rpx;
    border-radius: 40rpx;
    display: flex;
    justify-content: center;
    align-items: center;
}
.wrap-click{
    width: 32rpx;
    height: 32rpx;
    border-radius: 32rpx;
}
.wrap-click-box-click{
    border: 1px solid #e60214;
}
.wrap-click-click{
    background: #e60214;
}
.address-box{
    display: flex;
    justify-content: space-between;
    margin:30rpx;
    align-items: center;
    font-size: 28rpx;
}
.no-address{
    display: flex;
    justify-content: space-between;
    align-items: center;
    background: #fff;
    padding: 36rpx 30rpx;
}
.invoice-box{
    display: flex;
    justify-content: space-between;
    background: #fff;
    align-items: center;
    height: 80rpx;
    margin:30rpx 0;
    padding: 0 20rpx;
    font-size: 28rpx;
}
.mask{
    width: 100%;
    height: 100%;
    position: fixed;
    z-index: 1000;
    background: rgba(0, 0, 0, 0.5);
    top: 0;
    left: 0;
    display: flex;
    justify-content: center;
    align-items: center;
}
.content{
    background: #fff;
    width: 560rpx;
    height:900rpx;
    /* position: absolute;
    left: 0;
    top: 0;
    right: 0;
    bottom: 0; */
}
.page-section-gap{
    font-size: 28rpx;
    color: #333;
}
radio-group {
    display: flex;
    justify-content: space-between;
    margin:20rpx 20rpx;
}
.page-input{
    display: flex;
    align-items: center;
    border-bottom: 1px solid #eee;
}
.page-section-title{
    margin-left: 20rpx;
    font-size: 28rpx;
}
.placeholder-font{
    font-size: 28rpx;
}
.btn-area{
    width: 100%;
    margin-top: 60rpx;
}
.weui-input{
    margin-left: 12rpx;
    font-size: 28rpx;
}
.jiesuan-box {
    display: flex;
    justify-content: space-between;
    width: 100%;
    height: 100rpx;
    position: fixed;
    bottom: 0;
    left: 0;
    border-bottom: 1px solid #eee;
    border-top: 1px solid #eee;
    background-color: #fff;
    z-index: 999;
    align-items: center;
  }
  
  .jiesuan-box .to-pay-btn {
    width: 320rpx;
    text-align: center;
    line-height: 100rpx;
    background-color: #f60;
    font-size: 32rpx;
    color: #fff;
  }
  
  .jiesuan-box .to-pay-btn.no-select {
    background-color: #ccc;
  }
  
  .jiesuan-box  .left-price {
    display: flex;
    justify-content: space-between;
    line-height: 100rpx;
    font-size: 28rpx;
    box-sizing: border-box;
    width: 100%;
    justify-content: center;
  }
  .jiesuan-box .total {
    color: #e64340;
  }
  .ipx{
    height: 120rpx;
  }
  .ccc{
    background: #ccc;
  }
  .title-icon {
    display: inline-block;
    height: 32rpx;
    width: 60rpx;
    border: 1px solid #f60;
    text-align: center;
    line-height: 32rpx;
    color: #f60;
    font-size: 24rpx
  }