.clearfix:after {
    content: "";
    display: block;
    height: 0;
    clear: both;
    visibility: hidden;
}

.goods-container-list {
    display: flex;
    justify-content: space-between;
    flex-wrap: wrap;
    box-sizing: content-box;
    padding: 24rpx;
    background-color: #fff;
}

.goods-list-box {
    width: 350rpx;
    height: 520rpx;
    background-color: #fff;
    overflow: hidden;
    color: #333;
    position: relative;
}

.goods-list-item {
    width: 350rpx;
    height: 320rpx;
}

.pentagon {
    width: 32rpx;
    height: 32rpx;
}

.goods-container-list .goods-list-box:nth-child(2n+1) {
    overflow: hidden;
    box-sizing: border-box;
    border-bottom: 1px solid #f3f3f3 !important;
}

.goods-container-list .goods-list-box:nth-child(2n) {
    overflow: hidden;
    box-sizing: border-box;
    border-left: 1px solid #f3f3f3 !important;
    border-bottom: 1px solid #f3f3f3 !important;
    padding-left: 10rpx;
}

.goods-list-title {
    height: 80rpx;
    font-size: 28rpx;
    text-overflow: -o-ellipsis-lastline;
    overflow: hidden;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical;

}

