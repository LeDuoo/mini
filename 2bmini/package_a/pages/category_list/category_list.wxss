.search {
    width: 700rpx;
    height: 66rpx;
    display: block;
    box-sizing: border-box;
    margin: 16rpx auto 0;
}

.search input {
    display: block;
    box-sizing: border-box;
    background: #ECECEC;
    width: 700rpx;
    height: 66rpx;
    border-radius: 30rpx;
    padding-left: 32rpx;
    text-align: center;
    font-size: 28rpx;
}

.search image {
    width: 35rpx;
    height: 35rpx;
    position: absolute;
    top: 32rpx;
    left: 240rpx;
}

.active {
    height: 4rpx;
    width: 60rpx;
    background: #f60f32;
    margin: 14rpx auto;
}

.icon {
    height: 20rpx;
    font-size: 24rpx;
    color: #999
}

.iconActive {
    color: #333;
    font-weight: 600;
}

.recommendAction-box {
    display: flex;
    font-size: 28rpx;
    color: #666;
    margin-top: 40rpx
}

.recommendAction {
    display: flex;
    flex-direction: column;
}

.length {
    color: #999;
    font-size: 24rpx;
    margin: 20rpx 0 40rpx 0
}

.toDetailsTap {
    display: flex;
    height: 260rpx;
    border-bottom: 1px solid #f4f4f4
}

.PhoneCall-box {
    position: fixed;
    right: 0;
    bottom: 200rpx
}

.PhoneCall {
    width: 100rpx;
    font-size: 96rpx;
    color: #F60F39;
}