

page,
view,
image,
input,
textarea {
  display: block;
  box-sizing: border-box;
}

.container {
  background-color: #F2f2f2;
  min-height: 100%;
  padding-bottom: 100rpx;
}

.swiper-container {
  width: 100%;
  position: relative;
}

.swiper_box {
  width: 100%;
  height: 748rpx;
}

.goods-price {
  font-weight: 400;
  overflow: hidden;
  font-size: 30rpx;
  color: #f60;
  margin-left: 80rpx;
  width: 50%;
}

swiper-item image {
  width: 100%;
  display: inline-block;
  overflow: hidden;
  height: 748rpx;
}

.goods-info {
  background-color: #fff;
  padding: 35rpx 0;
  margin-bottom: 24rpx;
  width: 100%;
  position: relative;
}

.goods-info-fx {
  position: absolute;
  right: 35rpx;
  bottom: 60rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
  width: 130rpx;
}

.goods-info-fx .item {
  width: 50rpx;
  height: 50rpx;
  right: 130rpx;
  top: 100rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  font-size: 24rpx;
  background: #fff;
  z-index: 20;
}

.goods-info-fx .item image {
  width: 50rpx;
  height: 50rpx;
}

.goods-info-fx .item button {
  position: absolute;
  height: 100%;
  width: 100%;
  opacity: 0;
  z-index: 99;
}

.goods-info .goods-title {
  box-sizing: border-box;
  padding: 0 35rpx;
  font-size: 32rpx;
  line-height: 1.4;
  color: #000;
}

.goods-info .goods-share {
  box-sizing: border-box;
  padding: 0 35rpx;
  font-size: 25rpx;
  line-height: 1.4;
  color: #CC0000;
  margin-bottom: 28rpx;
}

.goods-profile {

  align-items: center;
}

.goods-profile .p {
  color: #f60;
  font-size: 32rpx;
  margin-left: 30rpx;
  width: 50%;
}

.goods-profile .p text {
  font-size: 30rpx;
}

.goods-profile .r {
  color: #ccc;
  font-size: 10pt;
  margin-left: 30rpx;
}

.row-arrow {
  width: 100vw;
  padding: 0 32rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
  height: 102rpx;
  font-size: 28rpx;
  line-height: 102rpx;
  background: #fff;
  margin-bottom: 32rpx;
}

.row-arrow image {
  width: 40rpx;
  height: 40rpx;
}

.goods-des-info {
  width: 100%;
  box-sizing: border-box;
  background-color: #fff;
}

.label-title {
  font-size: 28rpx;
  color: #000000;
  padding: 30rpx;
}

.goods-text {
  padding: 0 10rpx;
  font-size: 28rpx;
  color: #666666;
  line-height: 56rpx;
}

.goods-text image {
  width: 100%;
}

.btn-box {
  position: fixed;
  bottom: 0;
  left: 0;
  height: 96rpx;
  display: flex;
  line-height: 96rpx;
  text-align: center;
}

.btn-box view {
  width: 375rpx;
  color: #fff;
}

.isIpx {
  height: 112rpx;
  line-height: 96rpx;
}

.mask {
  width: 100%;
  height: 100%;
  background: rgba(0, 0, 0, 0.5);
  position: fixed;
  top: 0;
  left: 0;
}

.address {
  height: 600rpx;
  width: 100%;
  position: absolute;
  bottom: 0;
  left: 0;
  background: #fff;
}

.address-btn {
  background: #e60214;
  color: #fff;
  height: 80rpx;
  width: 100%;
  line-height: 80rpx;
  text-align: center;
  position: absolute;
  left: 0;
  bottom: 0;
}

.dialog_btn_buy {
  background: #999;
}

.dialog_btn_buy_f {
  background: #f60;
}

.buy-num-box {
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  background-color: rgba(0, 0, 0, 0.3);
  z-index: 99999999999;
}

.num-label {
  font-size: 26rpx;
  color: #000000;
}

.buy-num-box .num-box {
  display: flex;
  align-items: center;

}

.buy-num-box .num-box .hui {
  background-color: #f5f5f9;
}

.buy-num-box .num-box .num-jian,
.buy-num-box .num-box .num-input,
.buy-num-box .num-box .num-jia {
  width: 80rpx;
  height: 64rpx;
  line-height: 62rpx;
  text-align: center;
